{
	"name" : "goodmagma/laravel-translations",
	"description" : "Export Localization strings and translate it via Google Translate for Laravel",
	"keywords" : [
		"laravel",
		"translations",
		"translation",
		"export",
		"exporter",
		"json",
		"localization"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "Denis Torresan",
			"email" : "denis.torresan@gmail.com"
		}
	],
	"require" : {
		"php" : "^8.0",
		"ext-json" : "*",
		"illuminate/support" : "^8|^9|^10.0",
		"illuminate/translation" : "^8|^9|^10.0",
		"symfony/finder" : "^5|^6"
	},
	"require-dev" : {
		"nunomaduro/larastan" : "^1.0|^2.0",
		"phpunit/phpunit" : "^9.0",
		"orchestra/testbench" : "^6.0|^7.0|^8.0"
	},
	"autoload" : {
		"psr-4" : {
			"Goodmagma\\Translations\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Goodmagma\\Translations\\TranslationsServiceProvider"
			]
		}
	},
	"config" : {
		"sort-packages" : true
	},
	"minimum-stability" : "stable"
}